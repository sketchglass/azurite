{
  "name": "Azurite",
  "version": "0.1.0",
  "description": "Casual Painting App in Electron",
  "main": "dist/assets/main.js",
  "scripts": {
    "build:autogypi": "autogypi",
    "node-gyp:rebuild": "node-gyp rebuild --target=1.8.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "node-gyp:build": "node-gyp build --target=1.8.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "build:nativelib": "npm-run-all build:autogypi node-gyp:rebuild",
    "build:main": "webpack --config webpack.config.main.js",
    "build:renderer": "webpack",
    "build": "npm-run-all build:main build:renderer",
    "watch:main": "webpack --watch --config webpack.config.main.js",
    "devserver": "webpack-dev-server --hot",
    "watch": "npm-run-all --parallel watch:main devserver",
    "app": "electron . --devserver --development",
    "start": "npm-run-all build --parallel devserver app",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix 'src/**/*.{ts,tsx}'",
    "test:dev": "electron . --devserver --test",
    "pretest": "npm run build",
    "test": "electron . --test",
    "install": "npm run build:nativelib",
    "postinstall": "electron-rebuild -e node_modules/electron -w receive-tablet-event",
    "package": "ts-node scripts/package.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanchas116/azurite.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/seanchas116/azurite/issues"
  },
  "homepage": "https://github.com/seanchas116/azurite#readme",
  "private": true,
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/mocha": "^2.2.40",
    "@types/msgpack-lite": "^0.1.4",
    "@types/node": "^7.0.12",
    "@types/power-assert": "^1.4.29",
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "@types/webpack-env": "^1.13.0",
    "autogypi": "^0.2.2",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-glslify": "github:glslify/babel-plugin-glslify",
    "css-loader": "^0.28.0",
    "electron": "1.8.4",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^12.0.1",
    "electron-rebuild": "^1.5.7",
    "file-loader": "^0.11.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.1",
    "node-gyp": "^3.6.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^6.0.4",
    "power-assert": "^1.4.2",
    "resolve": "^1.3.2",
    "shelljs": "^0.7.7",
    "stack-source-map": "^1.0.6",
    "style-loader": "^0.16.1",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-fail-plugin": "^1.0.6"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "decamelize": "^1.2.0",
    "deep-assign": "^2.0.0",
    "deep-equal": "^1.0.1",
    "glslify": "^6.0.2",
    "keyboard-layout": "^2.0.11",
    "minimist": "^1.2.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.5",
    "msgpack-lite": "^0.1.26",
    "nbind": "^0.3.9",
    "paintgl": "github:sketchglass/paintgl#64926a07ab5eadbc97845ec417e1e2acdfc35714",
    "paintvec": "github:sketchglass/paintvec#2c5deb2c4cea6e2b22e43cb5bf8b96115dc97455",
    "promise-semaphore": "^0.2.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draggable-tree": "^0.2.0",
    "receive-tablet-event": "github:seanchas116/receive-tablet-event#8147b9191c67fb6c818ff9c6eab28e0e6fb699b7"
  }
}
